# MedWard Pro - Development Environment
# Copy this file and fill in your Firebase project details

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-dev-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project-dev.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-dev
VITE_FIREBASE_STORAGE_BUCKET=your-project-dev.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

# Use Firebase Emulators in development
VITE_USE_EMULATORS=true

# AI Features (optional - falls back to mock responses)
VITE_OPENAI_ENABLED=false

# App Configuration
VITE_APP_NAME=MedWard Pro (Dev)
VITE_APP_ENV=development

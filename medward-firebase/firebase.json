{
  "functions": {
    "source": "functions",
    "runtime": "nodejs18",
    "codebase": "default"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "pubsub": {
      "port": 8085
    },
    "database": {
      "port": 9000
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 4000
    },
    "auth": {
      "port": 9099
    },
    "ui": {
      "enabled": true
    }
  }
}
